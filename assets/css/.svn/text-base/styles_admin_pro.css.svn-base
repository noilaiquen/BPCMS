}
.blk-service li > p:after, .blk-search-term li > p:after{
    content: '';
    position: absolute;
    left: 0;
    bottom: -6px;
    width: 100%;
    height: 2px;
    background-color: rgba(0, 0, 0, 0.1);
}
.blk-service li > p:before, .blk-search-term li > p:before{
    content: '';
    position: absolute;
    right: -4px;
    bottom: 7px;
    width: 17px;
    height: 7px;
    background:url('../images/Photo_library/icon_dw.png') no-repeat;
	transition:all 0.8s;
	-webkit-transform: rotate(0deg);	
    -moz-transform: rotate(0deg);	
    -o-transform: rotate(0deg);	
    -ms-transform: rotate(0deg);	
    transform: rotate(0deg);	
}
