.item-dailt img{
	width:100%;
	border-radius:6px;
}
.item-dailt{position:relative;display:block;margin-top:15px;margin-bottom:15px;overflow:hidden;}
.item-dailt span{
	position:absolute;
	top:12px;
	right:12px;
	width:44px;
	height:42px;
	background:url('../images/dashboard/start_icon.png');
}
.item-dailt span.like-img{background:url('../images/dashboard/start_icon_act.png');}
.item-dailt figcaption{
	position:absolute;
	left:0;
	bottom:0;
	height:197px;
	width:100%;
	background-color:rgba(139, 108, 228, 0.72);
	color:#fff;
	text-align:center;
	opacity:0;
	-webkit-transition:0.6s;
	-moz-transition:0.6s;
	-ms-transition:0.6s;
	transition:0.6s;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}
.item-dailt figcaption h2{
	font-size:36px;
	padding-top:15px;
	-webkit-transition:all 0.7s;
	-moz-transition:all 0.7s;
	transition:all 0.7s;
	-webkit-transform:translateX(-50px);
	-ms-transform:translateX(-50px);
	-moz-transform:translateX(-50px);
	-o-transform:translateX(-50px);
	transform:translateX(-50px);
}
.item-dailt figcaption p{
	font-size:18px;
	font-family:'Roboto-Medium';
}
.item-dailt figcaption p:nth-child(odd){
	-webkit-transform:translateX(-50px);
	-ms-transform:translateX(-50px);
	-moz-transform:translateX(-50px);
	-o-transform:translateX(-50px);
	transform:translateX(-50px);
	-webkit-transition:all 0.6s;
	-moz-transition:all 0.6s;
	transition:all 0.6s;
}
.item-dailt figcaption p:nth-child(even){
	-webkit-transform:translateX(50px);
	-moz-transform:translateX(50px);
	-ms-transform:translateX(50px);
	-o-transform:translateX(50px);
	transform:translateX(50px);
	-webkit-transition:all 0.6s;
	-moz-transition:all 0.6s;
	transition:all 0.6s;
}
.item-dailt figcaption p:nth-child(4),
.item-dailt figcaption p:nth-child(5){
	font-family:inherit;
	opacity: .8;
}
.item-dailt figcaption button{
	width:90px;
	height:28px;
	background-color:#ff00ff;
	display:block;
	margin:3px auto 0;
	font-family:'Roboto-Medium';
	padding-top:5px;
	border: none;
}
.item-dailt:hover figcaption{opacity:1;}
.item-dailt:hover figcaption h2,
.item-dailt:hover figcaption p{
	-webkit-transform:translateX(0);
	-ms-transform:translateX(0);
	-o-transform:translateX(0);
	-moz-transform:translateX(0);
	transform:translateX(0);
}
